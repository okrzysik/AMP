Load_Boundary {
  name = "DirichletVectorCorrection"
    print_info_level = 1
    skip_params = FALSE
    setResidual = FALSE
    valuesType = 1
    isAttachedToVolumeOperator = FALSE

    number_of_ids = 1
    id_0 = 3
    number_of_dofs_0 = 1
    dof_0_0 = 2
    value_0_0 = 0.0
    //value_0_0 = 1.0e+8
}

NonlinearSolver{
  print_info_level = 7 
    max_iterations = 100
    max_error = 1e-10

    absoluteTolerance = 1.0e-15
    relativeTolerance = 1.0e-10
    stepTolerance = 1.0e-10
    maximumFunctionEvals = 1000
    usesJacobian=TRUE

    SNESOptions = "-snes_monitor -snes_type ls -snes_converged_reason -snes_ksp_ew -snes_ls_alpha 0.0001"

    LinearSolver{
      print_info_level = 1
        max_iterations = 10
        max_error = 1e-10
        ksp_type = "fgmres"
        absolute_tolerance = 1.0e-15
        relative_tolerance = 1.0e-11
        divergence_tolerance = 1.0e03
        max_krylov_dimension = 40
        uses_preconditioner=TRUE
        pc_type = "shell"
        pc_side = "RIGHT"

        KSPOptions = "-ksp_monitor -ksp_converged_reason"

        Preconditioner{
          print_info_level = 1
            PDE_equations = 3
            max_iterations = 1
            max_error = 1.0e-15
        }
    }
}

