mesh_file = "twentyBrick0"

NumberOfMeshes = 0

NumberOfLoadingSteps = 25

ExtractStressStrainData = TRUE

NonlinearMechanicsOperator {
    name="NonlinearBVPOperator"
    print_info_level = 1
    VolumeOperator = "MechanicsNonlinearFEOperator"
    BoundaryOperator = "DirichletVectorCorrection"
}

LinearMechanicsOperator {
  name="LinearBVPOperator"
    print_info_level = 1

    VolumeOperator = "MechanicsLinearFEOperator"
    BoundaryOperator = "DirichletMatrixCorrection"
}

Load_Boundary {
  name = "DirichletVectorCorrection"
    print_info_level = 1
    skip_params = FALSE
      setResidual = FALSE
      valuesType = 1
    isAttachedToVolumeOperator = FALSE
    
    number_of_ids = 4
    
    id_0 = 4
    number_of_dofs_0 = 1
    dof_0_0 = 2
    value_0_0 = 1.0e-1
    
    id_1 = 5
    number_of_dofs_1 = 1
    dof_1_0 = 2
    value_1_0 = 1.0e-1
    
    id_2 = 6
    number_of_dofs_2 = 1
    dof_2_0 = 2
    value_2_0 = 1.0e-1
    
    id_3 = 7
    number_of_dofs_3 = 1
    dof_3_0 = 2
    value_3_0 = 1.0e-1
}

NonlinearSolver{
  print_info_level = 2 
    max_iterations = 100
    max_error = 1e-10

    absolute_tolerance = 1.0e-15
    relativeTolerance = 1.0e-12
    stepTolerance = 1.0e-10
    maximumFunctionEvals = 1000
    usesJacobian=TRUE

    SNESOptions = "-snes_monitor -snes_type ls -snes_converged_reason -snes_ls_alpha 0.001"

    LinearSolver{
      print_info_level = 1
        max_iterations = 10
        max_error = 1e-10
        ksp_type = "fgmres"
        absolute_tolerance = 1.0e-15
        relative_tolerance = 1.0e-13
        divergence_tolerance = 1.0e03
        max_krylov_dimension = 40
        uses_preconditioner=TRUE
        pc_type = "shell"
        pc_side = "RIGHT"

        KSPOptions = "-ksp_monitor -ksp_converged_reason"

        Preconditioner{
          print_info_level = 1
            PDE_equations = 3
            max_iterations = 1
            max_error = 1.0e-15
        }
    }
}

MechanicsNonlinearFEOperator{
      name = "MechanicsNonlinearFEOperator"
        print_info_level = 1
        ActiveInputVariables {
          DISPLACEMENT = "displacement"
        }
      OutputVariable = "displacement"
        RESET_REUSES_RADIAL_RETURN = TRUE
        JACOBIAN_REUSES_RADIAL_RETURN = TRUE

        MechanicsElement {
          name="MechanicsNonlinearElement"
            USE_REDUCED_INTEGRATION = FALSE 
            FE_ORDER = "FIRST"  
            FE_FAMILY = "LAGRANGE"
            QRULE_TYPE = "QGAUSS"
            QRULE_ORDER = "DEFAULT"
        }
	LocalModel = "MechanicsMaterialModel"
    }

